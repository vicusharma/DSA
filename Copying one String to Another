public class Main {
    public static void main(String[] args) {
    int i = 0; 
    Scanner sc = new Scanner(System.in);
    String str = sc.nextLine();
    char y[] = str.toCharArray();
    int size = y.length;
        char a[] = new char[size];
        while(i != size){
            a[i] = y[i];
            ++i;
        }
        System.out.println(y);
        System.out.println(a);
    }
}
